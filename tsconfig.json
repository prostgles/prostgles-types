{
  "files": ["lib/index.ts"],
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es2020",
    "lib": [
      "es2022"
    ],
    "strict": true,
    "module": "commonjs",
    "removeComments": true,
    "sourceMap": true,
    "rootDir": "./lib",
    "outDir": "./dist",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "declaration": true,
    "declarationMap": true,
    "isolatedModules": true,
    "ignoreDeprecations": "5.0",
    "keyofStringsOnly": true,
    "noUncheckedIndexedAccess": true
  },
  "exclude": [
  ],
}